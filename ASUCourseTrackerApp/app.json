{
  "expo": {
    "name": "ASU Course Tracker",
    "slug": "asu-course-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#8C1D40"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asucoursetracker.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "aps-environment": "development"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#8C1D40"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.asucoursetracker.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#8C1D40",
          "defaultChannel": "default"
        }
      ]
    ],
    "description": "Track ASU courses and get notified when seats become available",
    "primaryColor": "#8C1D40",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "extra": {
      "eas": {
        "projectId": "341141ef-768c-4fe0-9e4c-6a05606c6315"
      }
    }
  }
}
